{
  "name": "server",
  "version": "1.0.0",
  "description": "production ready server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "server:dev": "nodemon server.js",
    "server:prod": "set NODE_ENV=production&&node server.js",
    "server:prod:win": "set NODE_ENV=production&&node server.js",
    "build": "npm install",
    "deploy:full": "npm run build && npm run deploy:s3",
    "deploy:s3": "node scripts/deploy-to-s3.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy:docker": "node scripts/deploy-docker.js",
    "setup:cost-reports": "node scripts/setup-cost-reports.js"

  },
  "author": "Me",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.485.0",
    "@aws-sdk/client-cost-explorer": "^3.817.0",
    "@aws-sdk/client-dynamodb": "^3.817.0",
    "@aws-sdk/client-eventbridge": "^3.817.0",
    "@aws-sdk/client-iam": "^3.817.0",
    "@aws-sdk/client-lambda": "^3.485.0",
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/lib-dynamodb": "^3.485.0",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cryptr": "^6.3.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "mongodb": "^6.12.0",
    "mongodb-connection-string-url": "^3.0.1",
    "mongoose": "^8.9.2",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
